    },
    "ImageUrl1": {
      "type": "string",
      "default": "null"
    },
    "shop": {
      "model": "shop",
      "via": "orders"
    },
    "ImageUrl2": {
      "type": "string",
      "default": "null"
    },
    "orderstatus": {
      "model": "orderstatus"
    }
  }
}